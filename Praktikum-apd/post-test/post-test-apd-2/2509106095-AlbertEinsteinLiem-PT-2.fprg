    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="nama, NIM" type="String" array="False" size=""/>
            <declare name="harga, diskonacer, diskonasus, diskonlenovo" type="Integer" array="False" size=""/>
            <declare name="hargaacer, hargaasus, hargalenovo" type="Integer" array="False" size=""/>
            <output expression="&quot;masukan nama lengkap&quot;" newline="True"/>
            <input variable="nama"/>
            <output expression="&quot;masukan NIM&quot;" newline="True"/>
            <input variable="NIM"/>
            <output expression="&quot;masukan harga laptop&quot;" newline="True"/>
            <input variable="harga"/>
            <assign variable="diskonacer" expression="harga * 0.05"/>
            <assign variable="diskonasus" expression="harga * 0.07"/>
            <assign variable="diskonlenovo" expression="harga * 0.10"/>
            <assign variable="hargaacer" expression="harga - diskonacer"/>
            <assign variable="hargaasus" expression="harga - diskonasus"/>
            <assign variable="hargalenovo" expression="harga - diskonlenovo"/>
            <output expression="nama &amp; &quot; dengan NIM &quot; &amp; NIM" newline="True"/>
            <output expression="&quot;Harga awal laptop: Rp&quot; &amp; harga" newline="True"/>
            <output expression="&quot;Jika membeli laptop Acer anda harus membayar: Rp&quot; &amp; hargaacer" newline="True"/>
            <output expression="&quot;Jika membeli laptop Asus anda harus membayar: Rp&quot; &amp; hargaasus" newline="True"/>
            <output expression="&quot;Jika membeli laptop Lenovo anda harus membayar: Rp&quot; &amp; hargalenovo" newline="True"/>
        </body>
    </function>
